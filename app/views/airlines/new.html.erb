<div class="container">
  <%= form_for @airline do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :link_url, 'Link URL' %>
    <%= f.text_field :link_url %>

    <%= f.label :logo_url, 'Logo URL' %>
    <%= f.text_field :logo_url %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.submit "Add Airline" %>
  <% end %>
</div>
