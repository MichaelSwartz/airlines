<h1>Users</h1>

<ul class="small-block-grid-3">
  <% @users.each do |user| %>
    <li>
      <a href="<%= admin_user_path(user) %>"><%= image_tag user.profile_photo, size: "250x250" , class: "airline_index"%></a>
      <a href="<%= admin_user_path(user) %>"><%= user.email %></a>
      <% if user.admin %>
        Admin
      <% end %>
    </li>
  <% end %>
</ul>

<%= paginate @users %>
